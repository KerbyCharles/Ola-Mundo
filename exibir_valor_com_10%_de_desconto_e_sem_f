valor = float(input('Informe o valor unitÃ¡rio ))
qtd = int(input('Informe a quantidade ))

total_comprada = valor * qtd

if total_comprada >= 100:
desc = total_comprada * 0.10
desconto_total = total_comprada - desc

print(f'O valor da compra com desconto R$ {desconto_total}')
print(f'o valor da compra sem desconto R$ {total_comprada}')